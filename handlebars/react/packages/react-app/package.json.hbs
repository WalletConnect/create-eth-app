    "apollo-boost": "^0.4.9",
    "apollo-client": "^2.6.10",
    "apollo-utilities": "^1.3.4",
    "chalk": "^4.1.1",
    "eslint": "^7.29.0",
    "eslint-plugin-flowtype": "^5.7.2",
    "graphql": "^15.5.0",
    "ipfs-http-client": "^50.1.2",
